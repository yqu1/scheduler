{
  "manifest_version": 2,

  "name": "class scheduler",
  "description": "This extension allows user to schedule classes on browser",
  "version": "1.0",

  "browser_action": {
    "default_icon": "app/images/icon.png",
    "default_popup": "app/index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://www.ratemyprofessors.com/*"
  ],
    "web_accessible_resources": [
    "http://www.ratemyprofessors.com/*"
  ]
}